{
    "version": "0.2.0",
    "configurations": [{
        "type": "node",
        "request": "launch",
        "name": "nest-graphql",
        "program": "${workspaceFolder}/src/main.ts",
        "autoAttachChildProcesses": true,
        "runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
        "preLaunchTask": "tsc: build - tsconfig.json",
        "outFiles": [
            "${workspaceFolder}/dist/**/*.js"
        ]
    }]
}
